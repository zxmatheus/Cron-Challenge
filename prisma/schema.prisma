generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model moeda {
  id         Int    @id @default(autoincrement())
  nome       String
  preco      Float
  moedaPreco String

  hora DateTime

  @@unique([nome, moedaPreco, hora])
}
